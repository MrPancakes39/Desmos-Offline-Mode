{
  "name": "desmos-offline-mode",
  "version": "1.0.0",
  "description": "Desmos Graphing Calculator as a Desktop App",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . desmos-offline-mode --overwrite --asar=true --platform=win32 --arch=ia32 --icon=resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=SH --version-string.FileDescription=SH --version-string.ProductName=\"Desmos | Offline Mode\"",
    "package-linux": "electron-packager . desmos-offline-mode --overwrite --asar=true --platform=linux --arch=x64 --icon=resources/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrPancakes39/Desmos-Offline-Mode.git"
  },
  "keywords": [
    "desmos",
    "calculator"
  ],
  "author": "Salman Hasan <salmannhassan39@gmail.com>",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/MrPancakes39/Desmos-Offline-Mode/issues"
  },
  "homepage": "https://github.com/MrPancakes39/Desmos-Offline-Mode#readme",
  "devDependencies": {
    "electron": "^13.6.1",
    "electron-packager": "^15.4.0"
  }
}
